{
  "name": "miaoxing/api",
  "type": "miaoxing-plugin",
  "description": "Miaoxing API Plugin",
  "keywords": [
    "miaoxing",
    "api"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.16.0"
  },
  "require-dev": {
    "miaoxing/dev": "^9.2.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Api\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 9.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 3.x-dev"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@php wei g:auto-completion"
    ]
  }
}
