{
  "name": "miaoxing/api",
  "type": "miaoxing-plugin",
  "description": "Miaoxing API Plugin",
  "keywords": [
    "miaoxing", "api"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^4.1.1"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Api\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
